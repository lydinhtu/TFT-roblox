-- Cấu hình cho tướng Cận chiến
local config = {
	IsRanged = false, -- Đánh gần
	AnimationID = "rbxassetid://507777826" -- ID chém kiếm
}

-- Kích hoạt bộ não
UnitAI.KhoiTao(script.Parent, config)
